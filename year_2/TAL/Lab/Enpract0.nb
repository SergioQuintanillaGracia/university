Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"CountOccurr", "[", 
    RowBox[{"l_", ",", " ", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "count", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"count", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
