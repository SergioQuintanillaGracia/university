Cell[TextData[{
 "Implement a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " module whose input is a word x and an integer k. The module should return \
the set of k-length segments of x. "
}], "ItemNumbered",
 CellChangeTimes->{
  3.587281372564268*^9, {3.58728148594456*^9, 3.587281550798246*^9}, {
   3.587283510258602*^9, 3.587283510357462*^9}, {3.587796072665926*^9, 
   3.587796153058509*^9}},ExpressionUUID->"bdaf780e-23bf-47f0-89c2-\
6ecc953d3bc3"],

Cell[TextData[StyleBox["Hint: The last position of x where it is possible to \
find a segment of length k is position n-k+1, where n denotes the length of \
the word.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, {
  3.587281707489728*^9, 3.587281913367154*^9}, {3.58779615496086*^9, 
  3.587796219659649*^9}},ExpressionUUID->"de502b5c-25bd-4213-b1e9-\
2b5493b0962b"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Segments", "[", 
    RowBox[{"x_", ",", " ", "k_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"l", ",", " ", "i"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"l", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "<=", " ", 
         RowBox[{
          RowBox[{"Length", "[", "x", "]"}], " ", "-", " ", "k", "+", "1"}]}],
         ",", " ", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"l", ",", 
           RowBox[{"Take", "[", 
            RowBox[{"x", ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", 
               RowBox[{"i", "+", "k", "-", "1"}]}], "}"}]}], "]"}]}], "]"}], 
         ";"}]}], "\[IndentingNewLine]", "]"}], ";", " ", 
      RowBox[{"(*", " ", "For", " ", "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "l", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], " ", 
  RowBox[{"(*", " ", "Segments", " ", "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
