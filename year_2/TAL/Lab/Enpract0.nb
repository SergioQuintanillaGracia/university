Cell[TextData[{
 "Implement a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " module that, given a list and two integers i and j, returns the list with \
the elements in positions i and j swapped."
}], "ItemNumbered",
 CellChangeTimes->{{3.587282722409523*^9, 3.587282769495165*^9}, {
   3.587795676248272*^9, 3.587795732205561*^9}, 
   3.587796445877137*^9},ExpressionUUID->"a1278df4-a6ca-4e3d-a102-\
ee5752f68ffd"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Swap", "[", 
    RowBox[{"l_", ",", " ", "i_", ",", " ", "j_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"temp", ",", "newL"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"newL", " ", "=", " ", "l"}], ";", "\[IndentingNewLine]", 
      RowBox[{"temp", " ", "=", " ", 
       RowBox[{"newL", "[", 
        RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"newL", "[", 
        RowBox[{"[", "i", "]"}], "]"}], " ", "=", " ", 
       RowBox[{"newL", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"newL", "[", 
        RowBox[{"[", "j", "]"}], "]"}], " ", "=", " ", "temp"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "newL", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Swap", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"1", ",", " ", "2", ",", " ", "3", ",", " ", "4", ",", " ", "5"}],
     "}"}], ",", " ", "2", ",", " ", "4"}], "]"}]}], "Input",
