Cell[BoxData[
 RowBox[{
  RowBox[{"MyPower", "[", 
   RowBox[{"base_", ",", "exponent_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", "result"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"result", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "exponent"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"result", "=", 
         RowBox[{"result", "*", "base"}]}], ";"}]}], "\[IndentingNewLine]", 
      "]"}], ";", 
     RowBox[{"(*", " ", "for", " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "result", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}], 
  RowBox[{"(*", " ", "MyPower", " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.587795315001014*^9, 
